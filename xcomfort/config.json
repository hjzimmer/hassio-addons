{
    "name": "xComfort-hajozi",
    "version": "0.68-b1",
    "slug": "xcomfort-hajozi",
    "description": "Eaton xComfort to (3rd party) MQTT gateway - test from hajozi",
    "url": "https://github.com/karloygard/hassio-addons",
    "startup": "application",
    "boot": "auto",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7",
        "i386"
    ],
    "privileged": [
        "SYS_RAWIO"
    ],
    "usb": true,
    "init": false,
    "map": [
        "config:rw"
    ],
    "services": [
        "mqtt:need"
    ],
    "options": {
        "mqtt_client_id": "xcomfort",
        "mqtt_host": "core_mosquitto",
	"mqtt_port": 1883,
        "mqtt_user": "",
        "mqtt_passwd": "",
        "datapoints_file": "",
        "eprom": true,
        "ha_discovery": true,
        "ha_discovery_prefix": "homeassistant",
        "ha_discovery_remove": false,
        "verbose": false,
        "use_hidapi": false,
        "eci_hosts": []
    },
    "schema": {
        "mqtt_client_id": "str",
        "mqtt_host": "str",
	"mqtt_port": "int",
        "mqtt_user": "str",
        "mqtt_passwd": "password",
        "datapoints_file": "str",
        "eprom": "bool",
        "ha_discovery": "bool",
        "ha_discovery_prefix": "str",
        "ha_discovery_remove": "bool",
        "verbose": "bool",
        "use_hidapi": "bool",
        "eci_hosts": ["str"]
    }
}
